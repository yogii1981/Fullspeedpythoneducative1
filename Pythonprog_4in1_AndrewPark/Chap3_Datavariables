# Addition
a = 2 + 4
print(a)

# Subtraction
l1 = 7 - 5
print(l1)

# multiplication
d1 = 9 * 789
print(d1)

b = 6

print(a == b)

c = a + b
print("Value of c is ", c)

c += a
print("Value of c is ", c)

c -= a
print("New value of c is", c)

i = 25
if i < 30 and i > 18:
    print("Condition is fulfllied")
else:
    print("Condition s not fulfilled")

# logial OR

i = 25
if i < 18 or i > 20:
    print("Condition is fulfilled")
else:
    print("Condition is fulfilled")
